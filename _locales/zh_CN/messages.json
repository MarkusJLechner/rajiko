{
    "manifest_app_description": {
        "message": "Rajiko是一个解除radiko.jp地区限制的工具。能从大阪、长崎，甚至海外听到东京、青森等地的电台。"
    }
    ,"confirm_button":{
        "message":"确认"
    }
    ,"refresh_alert":{
        "message":"请刷新radiko.jp的网页！"
    }
    ,"popup_title":{
        "message":"选择地区 - Rajiko"
    }
    ,"record_button_to_start":{
        "message":"开始录音"
    }
    ,"record_button_to_stop":{
        "message":"停止录音"
    }    
    ,"record_prepare":{
        "message":"没有正在播放的广播，已准备好为当前页面的$RADIO$录音。"
        ,"placeholders":{
            "radio":{
                "content" :"$1",
                "example" : "BAYFM"
            }
        }
    }
}